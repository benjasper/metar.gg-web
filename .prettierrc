{
	"printWidth": 120,
	"tabWidth": 4,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"jsxBracketSameLine": true,
	"arrowParens": "avoid",
	"filePath": "src",
	"requirePragma": false,
	"insertPragma": false,
	"proseWrap": "always",
	"plugins": ["prettier-plugin-tailwindcss"]
}
