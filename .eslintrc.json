{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["plugin:prettier/recommended", "plugin:solid/typescript"],
	"overrides": [],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint", "prettier", "solid"],
	"rules": {
		"linebreak-style": ["error", "unix"],
		"quotes": ["error", "single"],
		"semi": ["error", "never"],
		"prettier/prettier": "warn"
	}
}
